        "laravel-mix": "^2.0",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "adapterjs": "^0.15.3",
        "add": "^2.0.6",
        "dotenv": "^6.1.0",
        "dotenv-webpack": "^1.5.7",
        "express": "^4.16.4",
        "kurento-client": "^6.8.0",
        "kurento-utils": "^6.8.0",
        "minimist": "^1.2.0",
        "socket.io": "^2.2.0",
